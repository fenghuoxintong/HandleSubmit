package com.bjym.mobiledata.busbean;

public class BalanceResp {
	private float maxbalance;
	private float usedbalance;
	private float dayexpense;

	public float getMaxbalance() {
		return maxbalance;
	}

	public void setMaxbalance(float maxbalance) {
		this.maxbalance = maxbalance;
	}

	public float getUsedbalance() {
		return usedbalance;
	}

	public void setUsedbalance(float usedbalance) {
		this.usedbalance = usedbalance;
	}

	public float getDayexpense() {
		return dayexpense;
	}

	public void setDayexpense(float dayexpense) {
		this.dayexpense = dayexpense;
	}

}
